{
	"manifest_version": 2,
	"name": "WVCAT",
	"version": "0.1",
	"browser_action": {
		"default_icon": "./src/img/wvcat.png",
		"default_popup": "./src/popup.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": [
				"./src/js/wvcat.js",
				"./src/extension/content.js",
				"./src/js/plugin.js"
			]
		}
	],
	"background": {
		"scripts": ["./src/extension/background.js"]
	},
	"permissions": ["storage", "tabs"]
}
